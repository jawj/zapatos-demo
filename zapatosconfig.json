{
  "db": {
    "connectionString": "{{DB_URL}}"
  },
  "outExt": ".ts",
  "schemaJSDoc": true,
  "debugListener": false,
  "schemas": {
    "public": {
      "include": "*",
      "exclude": [
        "geography_columns",
        "geometry_columns",
        "raster_columns",
        "raster_overviews",
        "spatial_ref_sys"
      ]
    },
    "extra": {
      "include": "*",
      "exclude": []
    },
    "uk": {
      "include": "*",
      "exclude": []
    },
    "us": {
      "include": "*",
      "exclude": []
    }
  },
  "columnOptions": {
    "dimensions": {
      "metres": {
        "insert": "excluded",
        "update": "excluded"
      }
    }
  }
}
{
  "db": {
    "connectionString": "{{DB_URL}}"
  },
  "tsNameTransforms": true,
  "outExt": ".ts",
  "schemaJSDoc": false,
  "debugListener": false,
  "schemas": {
    "public": {
      "include": "*",
      "exclude": [
        "geographyColumns",
        "geometryColumns",
        "rasterColumns",
        "rasterOverviews",
        "spatialRefSys"
      ]
    },
    "extra": {
      "include": "*",
      "exclude": []
    },
    "UK": {
      "include": "*",
      "exclude": []
    },
    "US": {
      "include": "*",
      "exclude": []
    },
    "empty": {
      "include": "*",
      "exclude": []
    }
  },
  "columnOptions": {
    "dimensions": {
      "metres": {
        "insert": "excluded",
        "update": "excluded"
      }
    },
    "UK.constituencies": {
      "updatedAt": {
        "insert": "excluded",
        "update": "excluded"
      }
    }
  }
}